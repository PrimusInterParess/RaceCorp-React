@model RaceAllViewModel
@{
    ViewData["Title"] = "All Races";
    this.ViewData["action"] = "All";
    this.ViewData["controller"] = "Race";
}


@if (this.TempData.ContainsKey("Message"))
{
    <div class="alert alert-success">@this.TempData["Message"]</div>
}


@if (this.TempData.ContainsKey("MessageDeleted"))
{
    <div class="alert alert-success">@this.TempData["MessageDeleted"]</div>
}

<h1>@this.ViewData["Title"]</h1>
<hr />
<partial name="_RaceAllPartial" model="@Model" />


@if (Model.Races.Count() != 0)
{
    <partial name="_PagingPartial" model="@Model" />
}


